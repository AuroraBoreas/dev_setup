{
	"rust-cargo-abort": {
		"prefix": "rust-cargo-abort",
		"body": [
			"[profile.release]",
			"panic='abort'$0",
		],
		"description": "Rust, Cargo, make the resulting binary as small as possible, switch from `unwinding`(cleanup by Rust compiler) to `abort`(cleanup by OS)"
	},

	"python-flit-dependencies": {
		"prefix": "python-flit-dependencies",
		"body": [
			"dependencies = [",
			"\t\"${1:name}>=${2:6.2.0}\"",
			"]",
			"requires-python = \">=${3:3.5}\"\n",
			"[tool.flit.module]",
			"${4:name} = \"${5:module_name}\"\n",
			"[project.entry-points.\"${6:groupname}\"]",
			"${7:package} = \"${8:$7.model:function}\"\n",
			"[project.scripts]",
			"${9:script_name} = \"${10:package}.${11:model}:${12:function}\"",
		],
		"description": "Python, Flit, build installable packages"
	},

	"python-Flit-classifiers": {
		"prefix": "python-flit-classifiers",
		"body": [
			"\"Private :: Do Not Upload\",",
			"\"Framework :: Django\",",
			"\"Programming Language :: Python :: 3\",",
		],
		"description": "Python, Flit, build installable packages"
	},
}
