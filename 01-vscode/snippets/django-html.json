	"django-Chartjs-reuse-multiple-charts": {
		"prefix": "dj-chartjs-ReuseMultipleCharts",
		"body": [
			"var myCharts = [];",
			"function draw(canvasId, chartType, chartLabels, chartLabel, chartData, chartTitle) {",
			"\tconst ctx = document.getElementById(canvasId);",
			"\tvar newChart = new Chart(ctx, {",
			"\t\t...",
			"\t});",
			"\tmyCharts.push(newChart);",
			"};",
			"function destroyAllCharts() {",
			"\tmyCharts.forEach(function(chart) {",
			"\t\tchart.destroy();",
			"\t});",
			"\tmyCharts = [];",
			"};",
		],
		"description": "django-html, chartjs, reuse multiple charts on one page",
	},

