	"django-pagination": {
		"prefix": "dj-pagination",
		"body": [
			"<div class='container mt-3'>",
			"\t<div class='row my-5'>",
			"\t\t<div class='col-md-4 offset-md-3'>",
			"\t\t\t<nav aria-label='Page navigation'>",
			"\t\t\t\t<ul class='pagination justify-content-center'>",
			"\t\t\t\t\t{% if page_obj.has_previous %}",
			"\t\t\t\t\t<li class='page-item'><a class='page-link' href=\"?{{request.GET.urlencode}}&page={{page_obj.previous_page_number}}\">previous</a></li>",
			"\t\t\t\t\t{% endif %}",
			"\t\t\t\t\t{% for num in page_obj.paginator.page_range %}",
			"\t\t\t\t\t\t{% if num <= 3 %}",
			"\t\t\t\t\t\t<li class='page-item {% if page_obj.number == num %}active{% endif %}'><a class='page-link' href=\"?{{request.GET.urlencode}}&page={{num}}\">{{num}}</a></li>",
			"\t\t\t\t\t\t{% endif %}",
			"\t\t\t\t\t{% endfor %}",
			"\t\t\t\t\t{% if page_obj.has_next %}",
			"\t\t\t\t\t<li class='page-item'><a class='page-link' href=\"?{{request.GET.urlencode}}&page={{page_obj.next_page_number}}\">Next</a></li>",
			"\t\t\t\t\t<li class='page-item'><a class='page-link' href=\"?{{request.GET.urlencode}}&page={{page_obj.paginator.num_pages}}\">last &raquo;</a></li>",
			"\t\t\t\t\t{% endif %}",
			"\t\t\t\t</ul>",
			"\t\t\t</nav>",
			"\t\t</div>",
			"\t</div>",
			"</div>",
		],
		"description": "django-html, pagination",
	},

