	"django-form-wedge": {
		"prefix": "dj-form-widgettweaks",
		"body": [
			"{% load widget_tweaks %}\n",
			"{% for field in form %}",
			"<div class='fieldWrapper mb-1'>",
			"\t{{ field.errors }}",
			"\t{% if field.field.widget.input_type != 'checkbox' %}",
			"\t\t<label class='sr-only' for='{{form_field.atuo_id}}'>{{form_field.label}}</label>",
			"\t\t{{field|add_class:'form-control'}}",
			"\t{% else %}",
			"\t\t{{field.label_tag}}",
			"\t\t{{field}}",
			"\t{% endif %}",
			"</div>",
			"{% endfor %}",
		],
		"description": "django-html, django-widget-tweaks, form-snippet.html",
	},

